@page
@model PalaceServer.Pages.logsModel

<div class"table-responsive">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>CreationDate</th>
                <th>MachineName</th>
                <th>HostName</th>
                <th>ApplicationName</th>
                <th>Category</th>
                <th>Context</th>
                <th>Message</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.LogInfoList)
           {
            <tr>
                <td>@item.CreationDate</td>
                <td>@item.MachineName</td>
                <td>@item.HostName</td>
                <td>@item.ApplicationName</td>
                <td>@item.Category</td>
                <td>@item.Context</td>
                <td>@item.Message</td>
            </tr>
           }
        </tbody>
    </table>
</div>
